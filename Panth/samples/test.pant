import stdlib;

func range [u32 size] -> u32[]
    u32[] arr;
    arr.size = size;
    auto i = 0;
    while i < size do
        arr[i] = i;
    end

    return arr;
end

func copy [u8[] mut * a, u8 mut[] * b] -> nil
    for i in range(a:size) do
        (*a)[i] = (*b)[i];
    end
end

template class pair(T,U) is
    T t;
